<?xml version="1.0" encoding="UTF-8"?>
<inset
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="0.0px"
    android:insetRight="0.0px" >

    <selector>
        <item android:state_pressed="true">
            <shape>
                <gradient
                    android:angle="270.0"
                    android:endColor="#a0e0b071"
                    android:startColor="#a0a67637" />

                <corners
                    android:radius="3dip" />

                <stroke
                    android:width="1dp"
                    android:color="#a0a67637" />
            </shape>
        </item>
        <item>
            <shape>
                <gradient
                    android:angle="270.0"
                    android:endColor="#00ffffff"
                    android:startColor="#00ffffff" />

                <corners
                    android:radius="3dip" />

                <stroke
                    android:width="1dp"
                    android:color="#ffffff" />
            </shape>
        </item>
    </selector>
</inset>